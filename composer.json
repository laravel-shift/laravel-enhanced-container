{
    "name": "michael-rubel/laravel-container-calls",
    "description": "Provides syntax sugar for Laravel container calls",
    "keywords": [
        "michael-rubel",
        "laravel",
        "laravel-container-calls"
    ],
    "homepage": "https://github.com/michael-rubel/laravel-container-calls",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Rubel",
            "email": "contact@observer.name",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0",
        "sebastiaanluca/php-pipe-operator": "^4.0",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "nunomaduro/larastan": "^0.7.12",
        "orchestra/testbench": "^6.15",
        "permafrost-dev/coverage-check": "^1.2",
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "3.*",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "MichaelRubel\\ContainerCall\\": "src"
        },
        "exclude-from-classmap": [
            "vendor/laravel/framework/src/Illuminate/Container/Container.php",
            "vendor/laravel/framework/src/Illuminate/Container/BoundMethod.php"
        ],
        "files": [
            "src/Helpers/helpers.php",
            "src/Overrides/Container.php",
            "src/Overrides/BoundMethod.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MichaelRubel\\ContainerCall\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichaelRubel\\ContainerCall\\CallServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
